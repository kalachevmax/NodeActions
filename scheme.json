{
  "name": "semantic.node.actions",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "act",

  "modules": {
    "semantic.node.actions": {
      "src": [
        "act.js",
        "type.js",
        "type-provider.js",

        "dm/entity.js",
        "dm/entity-type.js",
        "dm/file.js",
        "dm/symbol/FileContent.js",

        "fs/dir/dir.js",
        "fs/dir/get/get.js",
        "fs/file/file.js",
        "fs/file/open-flag.js",
        "fs/node/node.js",
        "fs/node/is/is.js",
        "fs/tree/tree.js",
        "fs/stream/stream.js",
        "fs/file-link.js",
        "fs/directory-link.js",
        "proc/proc.js",
        "hash/hash.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "semantic.fm": "https://github.com/kalachevmax/semantic.fm",
    "semantic.dm": "https://github.com/kalachevmax/semantic.dm",
    "semantic.am": "https://github.com/kalachevmax/semantic.am",
    "semantic.utils": "https://github.com/kalachevmax/semantic.utils"
  }
}
