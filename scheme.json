{
  "name": "NodeActsLibrary",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "act",

  "modules": {
    "act": {
      "src": [
        "act.js",
        "fs/fs.js",
        "fs/stream/stream.js",
        "proc/proc.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "justFM": "https://github.com/kalachevmax/justFM"
  }
}
