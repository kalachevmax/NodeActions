{
  "name": "semantic.node.actions",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "act",

  "modules": {
    "semantic.node.actions": {
      "src": [
        "act.js",
        "fs/fs.js",
        "fs/type.js",
        "fs/dir/dir.js",
        "fs/dir/read/read.js",
        "fs/file/file.js",
        "fs/node/node.js",
        "fs/node/is/is.js",
        "fs/tree/tree.js",
        "fs/stream/stream.js",
        "fs/file-link.js",
        "fs/directory-link.js",
        "fs/type-provider.js",
        "proc/proc.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "semantic.fm": "https://github.com/kalachevmax/semantic.fm",
    "semantic.dm": "https://github.com/kalachevmax/semantic.dm",
    "semantic.am": "https://github.com/kalachevmax/semantic.am"
  }
}
