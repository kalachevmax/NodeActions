{
  "name": "NodeActsLibrary",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "act",

  "modules": {
    "act": {
      "src": [
        "act.js",
        "fs/fs.js",
        "fs/type.js",
        "fs/dir/dir.js",
        "fs/dir/get/get.js",
        "fs/file/file.js",
        "fs/node/node.js",
        "fs/node/is/is.js",
        "fs/tree/tree.js",
        "fs/stream/stream.js",
        "fs/file-link.js",
        "fs/directory-link.js",
        "fs/type-provider.js",
        "proc/proc.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "justFM": "https://github.com/kalachevmax/justFM",
    "justDM": "https://github.com/kalachevmax/justDM",
    "justAM": "https://github.com/kalachevmax/justAM"
  }
}
